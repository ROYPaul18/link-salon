generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Event {
  id        String   @id @default(uuid())
  time      DateTime
  duration  Int
  location  String
  image     String
  title     String
  createdAt DateTime @default(now()) 
  updatedAt DateTime @updatedAt
}

